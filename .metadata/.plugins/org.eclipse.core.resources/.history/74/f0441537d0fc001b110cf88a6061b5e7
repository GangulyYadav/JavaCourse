import javax.swing.*;
class setBoundExample extends JFrame
{
	JPanel pan;
	JLabel lname,lage;
	JTextField tname,tage;
	JButton  submit,cancel;
	
	setBoundExample()
	{
		super("Login form");
		pan=new JPanel();
		pan.setBounds(null);
		add(pan);
		lname=new JLabel("Enter name");lage=new JLabel("Enter age");
		tname=new JTextField(20);tage=new JTextField(20);
	}
}
	
public class setBound {

}
